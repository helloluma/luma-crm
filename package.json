{
  "name": "real-estate-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run src/**/*.test.{ts,tsx}",
    "test:integration": "vitest run src/test/integration/**/*.test.{ts,tsx}",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:mobile": "playwright test tests/e2e/mobile-responsiveness.spec.ts",
    "test:e2e:cross-browser": "playwright test tests/e2e/cross-browser.spec.ts",
    "test:e2e:visual": "playwright test tests/e2e/visual-regression.spec.ts",
    "test:e2e:performance": "playwright test tests/e2e/performance.spec.ts",
    "test:smoke": "playwright test tests/e2e/smoke.spec.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:production": "npm run test:smoke",
    "security:audit": "npm audit --audit-level moderate && node scripts/security-audit.js",
    "security:ssl-check": "node scripts/ssl-check.js",
    "security:headers-check": "node scripts/headers-check.js",
    "lighthouse:ci": "lhci autorun",
    "test:load": "node scripts/load-test.js",
    "test:db-performance": "node scripts/db-performance-test.js",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "db:setup": "node scripts/setup-database.js",
    "db:seed": "node scripts/setup-database.js --seed",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db push",
    "db:rollback": "supabase db reset --linked",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.888.0",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "express-rate-limit": "^8.1.0",
    "google-auth-library": "^10.3.0",
    "googleapis": "^159.0.0",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.28.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "resend": "^6.1.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
